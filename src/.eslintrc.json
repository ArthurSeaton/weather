{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "process": false
  },
  "rules": {
    "semi": "error",
    "quotes": ["error", "single"]
  }
}
